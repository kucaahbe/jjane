<h1>Editing attached_file</h1>

<% form_for @attached_file do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @attached_file %> |
<%= link_to 'Back', attached_files_path %>
