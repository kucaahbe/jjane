<% form_for @user, :html => { :class => 'jjane' } do |f| %>
  <%= f.error_messages %>
  <p>
  <%= f.label :name, t(:label_name) %><br />
  <%= f.text_field :name %>
  </p>
  <p>
  <%= f.label :email, t(:label_email) %><br />
  <%= f.text_field :email %>
  </p>
  <p>
  <%= f.label :password, t(:label_password) %><br />
  <%= f.password_field :password %>
  </p>
  <p>
  <%= f.label :password_confirmation, t(:label_password_confirmation) %><br />
  <%= f.password_field :password_confirmation %>
  </p>
  <p>
  <%= f.label :group, t(:label_group) %><br />
  <%= f.select :group_id,  options_from_collection_for_select(Group.all, 'id', 'name') %>
  </p>
  <p>
  <%= f.submit t(:button_save) %> |
  <%= link_to_cancel users_path %>
  </p>
<% end %>
